    "version": "0.2.0",
    "configurations": [
        {
            "name": "lint-metadata",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["E:\\music","-v"]
        },
        {
            "name": "generate-random-playlist",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-l","ratings-list.txt","-o","playlist.m3u","-g","Blues","-g","Rock","-g","Pop","-r","150","-y","1970-1979","--all"]
        },
        {
            "name": "generate-metadata-list",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-o","ratings-list-windows.txt", "E:\\music","-z"]
        },
        {
            "name": "generate-metadata-list (Linux)",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-o","ratings-list-linux.txt", "E:\\music","-v","-x","E:/music/","-p","/mnt/music/Albums","-z"]
        },
        {
            "name": "fix-playlists",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["-p","..\\Playlists\\world-music-afro-arabic.m3u","-o","world-music-afro-arabic.m3u","-l","all1.txt","-x","/mnt/music/Albums/"]
        },
        {
            "name": "flac-to-mp3",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["e:\\new\\albums","-o","-d"]
        },
        {
            "name": "search-mbz-ratings",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["\\music","-s","http://192.168.2.90:5000","-v"]
        },
        {
            "name": "fix-ytm-titles",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["\\YTM.Playlists"]
        },
        {
            "name": "extract-covers",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["\\\\192.168.2.40\\books\\audiobooks"]
        },
        {
            "name": "group-actions",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["E:\\music","copy","-t","Medium","-t","XSmall","-t","Small","-d","F:\\Test","-f","bat","-o","do-copy.bat","-l","ratings-list-windows.txt"]
        }
    ]
}